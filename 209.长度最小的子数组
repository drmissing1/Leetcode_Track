class Solution(object):
    def minSubArrayLen(self, target, nums):
        """
        :type target: int
        :type nums: List[int]
        :rtype: int
        """
        sumnum = 0
        left = 0
        min_len = len(nums) + 1
        for right in range(0,len(nums)):
            if nums[left] >= target:
                return 1
            sumnum += nums[right]
            while sumnum >= target:
                min_len = min(min_len , right - left + 1)
                sumnum -= nums[left]
                left += 1
        if min_len == len(nums) + 1:
            return 0
        else: return min_len
