class Solution(object):
    def search(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        left , right = 0 , len(nums)-1
        while True:
            mid = (left + right) / 2
            if left > right:
                return -1
            if target == nums[mid]:
                return mid
            elif target > nums[mid]: left = mid + 1
            elif target < nums[mid]: right = mid - 1
